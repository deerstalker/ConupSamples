<html>

	<head>
		<script src="../base64.js" type="text/javascript"></script>
		<script type="text/javascript">
			var b = new Base64();
			var str = b.encode("["hello"]");
			alert("base64 encode:" + str);
			str = b.decode(str);
			alert("base64 decode:" + str);
		</script>
	</head>
	<body>
	</body>
</html>

